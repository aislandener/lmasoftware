<p id="notice"><%= notice %></p>

<h1>Listing Contas Pagars</h1>

<table class="table table-stripped">
  <thead>
    <tr>
      <th>Dataemissao</th>
      <th>Valor</th>
      <th>Vencimento</th>
      <th>Juros</th>
      <th>Multa</th>
      <th>Fornecedor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contas_pagars.each do |contas_pagar| %>
      <tr>
        <td><%= contas_pagar.dataEmissao %></td>
        <td><%= contas_pagar.valor %></td>
        <td><%= contas_pagar.vencimento %></td>
        <td><%= contas_pagar.juros %></td>
        <td><%= contas_pagar.multa %></td>
        <td><%= contas_pagar.Fornecedor.nome %></td>
        <td><%= link_to 'Show', contas_pagar %></td>
        <td><%= link_to 'Edit', edit_contas_pagar_path(contas_pagar) %></td>
        <td><%= link_to 'Destroy', contas_pagar, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

