<p id="notice"><%= notice %></p>

<h1>Listing Telefone Fornecedors</h1>

<table class="table table-stripped">
  <thead>
    <tr>
      <th>Ddd</th>
      <th>Telefone</th>
      <th>Fornecedor</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @telefone_fornecedors.each do |telefone_fornecedor| %>
      <tr>
        <td><%= telefone_fornecedor.ddd %></td>
        <td><%= telefone_fornecedor.telefone %></td>
        <td><%= telefone_fornecedor.Fornecedor %></td>
        <td><%= link_to 'Show', telefone_fornecedor %></td>
        <td><%= link_to 'Edit', edit_telefone_fornecedor_path(telefone_fornecedor) %></td>
        <td><%= link_to 'Destroy', telefone_fornecedor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

